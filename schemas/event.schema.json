{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Event",
  "type": "object",
  "properties": {
    "event": {
      "type": "string"
    },
    "isTestEvent": {
      "type": "boolean"
    },
    "eventType": {
      "type": "string"
    },
    "clientId": {
      "type": "string"
    },
    "id": {
      "type": "string"
    },
    "userAgent": {
      "type": "string"
    },
    "userIp": {
      "type": "string"
    },
    "sentAt": {
      "type": "string"
    },
    "userId": {
      "type": "string"
    },
    "sessionId": {
      "type": "string"
    },
    "fingerprintJsId": {
      "type": "string"
    },
    "fingerprintJsData": {
      "type": "object"
    },
    "timestamp": {
      "type": "integer"
    },
    "anonymousId": {
      "type": "string"
    },
    "originalTimestamp": {
      "type": "integer"
    },
    "utm": {
      "type": "object",
      "properties": {
        "utm_campaign": {
          "type": "string"
        },
        "utm_content": {
          "type": "string"
        },
        "utm_medium": {
          "type": "string"
        },
        "utm_source": {
          "type": "string"
        }
      }
    },
    "clid": {
      "type": "object",
      "properties": {
        "fbclid": {
          "type": "string"
        },
        "gclid": {
          "type": "string"
        },
        "mclid": {
          "type": "object",
          "properties": {
            "campaign": {
              "type": "string"
            },
            "adset": {
              "type": "string"
            },
            "ad": {
              "type": "string"
            },
            "timestamp": {
              "type": "integer"
            }
          },
          "required": ["campaign", "adset", "ad", "timestamp"]
        }
      }
    },
    "cookies": {
      "type": "object",
      "properties": {
        "_fbp": {
          "type": "string"
        },
        "_fbc": {
          "type": "string"
        },
        "_ga": {
          "type": "string"
        },
        "_gid": {
          "type": "string"
        },
        "_gcl_au": {
          "type": "string"
        },
        "_clck": {
          "type": "string"
        }
      }
    },
    "context": {
      "type": "object",
      "properties": {
        "page": {
          "type": "object",
          "properties": {
            "referrerUrl": {
              "type": "string"
            },
            "urlPath": {
              "type": "string"
            },
            "urlTitle": {
              "type": "string"
            },
            "host": {
              "type": "string"
            },
            "search": {
              "type": "string"
            }
          }
        },
        "library": {
          "type": "object",
          "properties": {
            "wrapperName": {
              "type": "string"
            }
          },
          "required": ["wrapperName"]
        }
      }
    },
    "location": {
      "type": "object",
      "properties": {
        "countryCode:": {
          "type": "string"
        },
        "city": {
          "type": "string"
        },
        "state": {
          "type": "string"
        },
        "country": {
          "type": "string"
        },
        "zipCode": {
          "type": "string"
        }
      }
    },
    "customerData": {
      "type": "object",
      "properties": {
        "email": {
          "type": "string"
        },
        "firstName": {
          "type": "string"
        },
        "lastName": {
          "type": "string"
        },
        "phone": {
          "type": "string"
        },
        "dateOfBirth": {
          "type": "string"
        },
        "gender": {
          "type": "string"
        }
      }
    },
    "shopping_data": {
      "type": "object",
      "properties": {
        "currency": {
          "type": "string"
        },
        "total_price": {
          "type": "number"
        },
        "order": {
          "type": "object"
        },
        "products": {
          "type": "array",
          "items": [
            {
              "type": "object",
              "properties": {
                "item_id": {
                  "type": "string"
                },
                "item_name": {
                  "type": "string"
                },
                "coupon": {
                  "type": "string"
                },
                "currency": {
                  "type": "string"
                },
                "discount": {
                  "type": "number"
                },
                "item_brand": {
                  "type": "string"
                },
                "item_category": {
                  "type": "string"
                },
                "price": {
                  "type": "number"
                },
                "quantity": {
                  "type": "integer"
                }
              },
              "required": ["item_id", "currency", "price", "quantity"]
            }
          ]
        }
      }
    },
    "customData": {
      "type": "object"
    }
  },
  "required": [
    "event",
    "eventType",
    "clientId",
    "id",
    "sentAt",
    "sessionId",
    "timestamp",
    "originalTimestamp",
    "context"
  ]
}
